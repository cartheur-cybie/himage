			JR L1310 ; reference (DoMoveMouth)

L1308:
			LDB A,(var_MouthFutz)
			SLLB 1,A
			JR L1310 ; reference (DoMoveMouth)

L1309:
			DB 021H,0 ; FAKE OP LDB A,0
L1310:
			LD (var_mouthValB),A
			LDB A,(var_bCtrlMouth)
			AND A,0FH
			JR Z,L1307
			CP A,0FH
			JR NZ,L1311
			LDB A,(mirrorP7)
			OR A,10H
			AND A,0DFH
			LD (mirrorP7),A
			LDB (var_countdown1Hz_c),04H
			SET 2,(var_596)
			LDB (var_599),0
			LDB (var_5A5),1
			SET 1,(var_583)
			LDB (var_mouthValB),0FFH
			JR L1312 ; reference (DoMoveMouth)

L1307:
			LDB A,(mirrorP7)
			OR A,30H
			LD (mirrorP7),A
			RES 1,(var_583)
			LDB (var_mouthValB),0FFH
			JR L1312 ; reference (DoMoveMouth)

L1311:
			SET 2,(var_bitsMovingHNMT)
			LDB (var_58F),07H
			LDB (var_countdown1Hz_c),04H
			SET 1,(var_583)
			LDB (var_5A5),1
L1312:
			RET ; reference (DoMoveMouth)

