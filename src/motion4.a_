L1670:
			BIT 1,(var_596)
			JR Z,L1273
			BIT 0,(P9)
			JR Z,L1273
			RES 3,(var_583)
			RES 1,(var_596)
			LDB A,(mirrorP7)
			OR A,2
			LD (mirrorP7),A
			LDB A,(mirrorP6)
			OR A,80H
			LD (mirrorP6),A
			LD (P6),A
			LDB (var_neckValB),0FFH
			LDB (var_neckValA),0
			LDB (var_5A5),02H
L1273:
			BIT 0,(var_596)
			JP Z,L1274
			CPB (var_597),5
			JR NC,L1275
			CPB (var_597),2
			JR NC,L1276
			CPB (var_597),1
			JR Z,L1277
			BIT 1,(P9)
			JR NZ,L1274
			LDB (var_2EF),1
			LDB (var_597),1
			LDB A,(mirrorP7)
			OR A,8
			AND A,0FBH
			LD (mirrorP7),A
			LD (P7),A
			JR L1274 ; reference (L1670)

L1277:
			BIT 1,(P9)
			JR Z,L1274
			LDB (var_597),02H
			LDB A,(mirrorP7)
			OR A,4
			AND A,0F7H
			LD (mirrorP7),A
			LD (P7),A
			JR L1274 ; reference (L1670)

L1276:
			INCB 1,(var_597)
			JR L1274 ; reference (L1670)

L1275:
			RES 2,(var_583)
			RES 0,(var_596)
			LDB (var_597),0
			LDB A,(mirrorP7)
			OR A,0CH
			LD (mirrorP7),A
			LD (P7),A
			LDB (var_2EF),0
			LDB (var_headValB),0FFH
			LDBB var_headValA,var_HeadFutz
			LDB (var_5A4),02H
L1274:
			BIT 2,(var_596)
			JR Z,L1278
			CPB (var_599),5
			JR NC,L1279
			CPB (var_599),1
			JR NC,L1280
			BIT 2,(P9)
			JR Z,L1278
			LDB A,(mirrorP7)
			OR A,30H
			LD (mirrorP7),A
			LDB (var_599),1
			JR L1278 ; reference (L1670)

L1280:
			INCB 1,(var_599)
			JR L1278 ; reference (L1670)

L1279:
			RES 1,(var_583)
			RES 2,(var_596)
			LDB (var_mouthValB),0FFH
			LDB (var_mouthValA),0
			LDB (var_5A3),02H
L1278:
			BIT 3,(var_596)
			JR Z,L1281
			CPB (var_countdown1Hz_d),3
			JR C,L1282
			BIT 3,(P9)
			JR Z,L1281
L1282:
			RES 0,(var_583)
			RES 3,(var_596)
			RES 0,(var_598)
			LDB A,(mirrorP7)
			OR A,0C0H
			LD (mirrorP7),A
			LDB (var_tailValB),0FFH
			LDBB var_tailValA,var_TailFutz
			LDB (var_5A6),02H
L1281:
			RET ; reference (L1670)

; ------------------------------


ProcessFutzControl: ; 64Hz - after HNMT processing
			BIT 0,(var_FutzControl)
			JR NZ,L1283
			RET ; reference (ProcessFutzControl)

L1283:
			BIT 5,(var_FutzControl) ; head
			JR NZ,L1284
			BIT 6,(var_FutzControl) ; mouth
			JR NZ,L1285
			BIT 7,(var_FutzControl) ; tail
			JR NZ,L1286
			INCB 1,(var_NeckFutz)
			RET ; reference (ProcessFutzControl)

L1284:
			INCB 1,(var_HeadFutz)
			RET ; reference (ProcessFutzControl)

L1285:
			INCB 1,(var_MouthFutz)
			RET ; reference (ProcessFutzControl)

L1286:
			INCB 1,(var_TailFutz)
			RET ; reference (ProcessFutzControl)

