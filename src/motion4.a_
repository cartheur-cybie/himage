DoMoveTail:
			AND A,60H
			JP Z,L1313
			CP A,20H
			JR Z,L1314
			CP A,40H
			JR Z,L1315
			LDB A,(var_TailFutz)
			SLLB 1,A
			DEC 6,A
			DB 022H,0 ; FAKE OP LDB B,0
			LDB C,(wvar_325)
			DIV BC,A
			LDB A,B
			JR L1316 ; reference (DoMoveTail)

L1314:
			LDB A,(var_TailFutz)
			SLLB 1,A
			DEC 6,A
			JR L1316 ; reference (DoMoveTail)

L1315:
			DB 021H,0 ; FAKE OP LDB A,0
L1316:
			LD (var_tailValB),A
			LDB A,(var_bCtrlTail)
			AND A,0FH
			JR Z,L1313
			CP A,0FH
			JR NZ,L1317
			LDB A,(var_TailFutz)
			CP (var_tailValA),A
			JR Z,L1313
			JR C,L1318
			LDB A,(mirrorP7)
			OR A,80H
			AND A,0BFH
			LD (mirrorP7),A
			LDB (var_59A),03H
			LDB (var_598),0
			JR L1319 ; reference (DoMoveTail)

L1318:
			LDB A,(mirrorP7)
			OR A,40H
			AND A,7FH
			LD (mirrorP7),A
			LDB (var_59A),03H
			LDB (var_598),1
L1319:
			LDB (var_countdown1Hz_d),04H
			SET 3,(var_596)
			LDB (var_5A6),1
			SET 0,(var_583)
			LDB (var_tailValB),0FFH
			JR L1320 ; reference (DoMoveTail)

L1313:
			LDB A,(mirrorP7)
			OR A,0C0H
			LD (mirrorP7),A
			RES 0,(var_583)
			LDB (var_tailValB),0FFH
			JR L1320 ; reference (DoMoveTail)

L1317:
			SET 3,(var_bitsMovingHNMT)
			LDB (var_58F),07H
			LDB (var_countdown1Hz_d),04H
			SET 0,(var_583)
			LDB (var_5A6),1
L1320:
			RET ; reference (DoMoveTail)

