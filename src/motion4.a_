Save4Parts:
			CALL SaveNeckData
			CALL SaveHeadData
			CALL SaveMouthData
			CALL SaveTailData
			RET

; ------------------------------


DoMove4Parts:
			LDB A,(var_bCtrlNeck)
			CALL DoMoveNeck
			LDB A,(var_bCtrlHead)
			CALL DoMoveHead
			LDB A,(var_bCtrlMouth)
			CALL DoMoveMouth
			LDB A,(var_bCtrlTail)
			CALL DoMoveTail
			RET ; reference (DoMove4Parts)

; ------------------------------


SaveNeckData:
			LDB A,(XIY+)
			LD (var_bCtrlNeck),A
			CPB (var_2E2),0
			JR Z,DoMoveNeck
			RET ; reference (SaveNeckData)

; ------------------------------


DoMoveNeck:
			AND A,60H
			JP Z,L1295
			CP A,20H
			JR Z,L1296
			CP A,40H
			JR Z,L1297
			LDB A,(var_NeckFutz)
			DEC 2,A
			DB 022H,0 ; FAKE OP LDB B,0
			LDB C,(wvar_325) ; random (512Hz)
			DIV BC,A
			LDB A,B
			JR L1298 ; reference (DoMoveNeck)

L1296:
			LDB A,(var_NeckFutz)
			DEC 2,A
			JR L1298 ; reference (DoMoveNeck)

L1297:
			DB 021H,0 ; FAKE OP LDB A,0
L1298:
			LD (var_neckValB),A
			LDB A,(var_bCtrlNeck)
			AND A,0FH
			JR Z,L1295
			CP A,0FH
			JR NZ,L1299
			LDB A,(mirrorP6)
			OR A,80H
			LD (mirrorP6),A
			LD (P6),A
			LDB A,(mirrorP7)
			AND A,0FDH
			LD (mirrorP7),A
			LDB (var_countdown1Hz_b),04H
			SET 1,(var_596)
			LDB (var_5A5),1
			SET 3,(var_583)
			LDB (var_neckValB),0FFH
			JR L1300 ; reference (DoMoveNeck)

L1295:
			LDB A,(mirrorP7)
			OR A,2
			LD (mirrorP7),A
			LDB A,(mirrorP6)
			OR A,80H
			LD (mirrorP6),A
			RES 3,(var_583)
			LDB (var_neckValB),0FFH
			JR L1300 ; reference (DoMoveNeck)

L1299:
			SET 1,(var_bitsMovingHNMT)
			LDB (var_58F),07H
			LDB (var_countdown1Hz_b),04H
			SET 3,(var_583)
			LDB (var_5A5),1
L1300:
			RET ; reference (DoMoveNeck)

; ------------------------------


SaveHeadData:
			LDB A,(XIY+)
			LD (var_bCtrlHead),A
			CPB (var_2E2),0
			JR Z,DoMoveHead
			RET ; reference (SaveHeadData)

; ------------------------------


DoMoveHead:
    ;; xTTx NNNN
    ;; TT = type
    ;; NNNN = count (1/64 sec)
			AND A,60H ; 0110 0000
			JP Z,L1301 ; off
			CP A,20H
			JR Z,L1302 ; use variable
			CP A,40H
			JR Z,L1303 ; use zero

            ;; 60H - 

			LDB A,(var_HeadFutz)
			SLLB 1,A
			DB 022H,0 ; FAKE OP LDB B,0

			LDB C,(wvar_325) ; random (bumped 512/sec)
			DIV BC,A
			LDB A,B
			JR L1304 ; reference (DoMoveHead)

L1302:
			LDB A,(var_HeadFutz)
			SLLB 1,A
			JR L1304 ; reference (DoMoveHead)

L1303:
			DB 021H,0 ; FAKE OP LDB A,0
L1304:
        ;; Head = right/left head turn
			LD (var_headValB),A
			LDB A,(var_bCtrlHead)
			AND A,0FH ; lower 4 bits = time value (64Hz)
			JR Z,L1301 ; off

			CP A,0FH
			JR NZ,L1305 ; 15

            ; fwd (left)
			LDB A,(mirrorP7)
			OR A,4
			AND A,~8
			LD (mirrorP7),A

			LDB (var_597),0
			LDB (var_countdown1Hz_a),04H
			SET 0,(var_596)
			LDB (var_5A4),1
			SET 2,(var_583)
			LDB (var_headValB),0FFH
			JR L1306 ; reference (DoMoveHead)

L1301:
			LDB A,(mirrorP7)
			OR A,0CH ; off
			LD (mirrorP7),A

			RES 2,(var_583)
			LDB (var_headValB),0FFH
			JR L1306 ; reference (DoMoveHead)

L1305:
			SET 0,(var_bitsMovingHNMT) ; head moving
			LDB (var_58F),07H
			LDB (var_countdown1Hz_a),04H
			SET 2,(var_583)
			LDB (var_5A4),1
L1306:
			RET ; reference (DoMoveHead)

